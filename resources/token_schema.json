{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "$id": "https://www.regulad.xyz/ColdCaller/resources/token_schema.json",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "pattern": "([a-zA-Z0-9_]{24})\\.([a-zA-Z0-9_]{6})\\.([a-zA-Z0-9_]{27})"
      },
      "password": {
        "type": "string"
      }
    },
    "required": [
      "token",
      "password"
    ]
  }
}
